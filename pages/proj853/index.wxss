/** pages/proj853/index.wxss **/


.svgMain {
  width: 150rpx;
  height: 150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  /* transform-origin: center; */
}

.fullScreen {
  width: 100%;
  height: 100%;
  transform-origin: center;
}

#svgDev {
  opacity: 0.6;
}

.infoArea {
  position: fixed;
  right: 20rpx;
  /* display: block; */
  z-index: 99600;
  color: #003380;
}

.infoLine {
  text-align: right;
  font-size: 20rpx;
  max-width: 240rpx;
}

.tabBar {
  position: fixed;
  /* display: block; */
  z-index: 99900;
  text-align: center;
  left: 75rpx;
  bottom: env(safe-area-inset-bottom);
  width: 600rpx;
  height: 150rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.tabBtn {
  margin: 10rpx;
  margin-top: 0;
  margin-bottom: 0;
  width: 120rpx;
  height: 150rpx;
  text-align: center;
  border: 5rpx solid #003380;
  border-radius: 10rpx;
  background-color: white;
}

.tabBtnIcon {
  margin: 15rpx;
  margin-bottom: 5rpx;
  width: 80rpx;
  height: 80rpx;
}

.tabBtnText {
  font-size: 20rpx;
  color: #003380
}

.mask {
  position: fixed;
  z-index: 99700;
  background-color: white;
  opacity: 0.4;
}

.msgBox {
  position: fixed;
  z-index: 99800;
  width: 650rpx;
  left: 50rpx;
  text-align: center;
  border: 5rpx solid #003380;
  border-radius: 10rpx;
  background-color: white;
}

.msgTitle {}

.msgText {}